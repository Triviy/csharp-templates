{
  "$schema": "http://json.schemastore.org/template",
  "author": "CompanyName",
  "classifications": [ "CompanyName", "System.API", "WebAPI" ],
  "name": "CompanyName.System.API",
  "shortName": "cnsystemapi",
  "sourceName": "TemplateName",
  "defaultName": "NewTemplate",
  "identity": "CompanyName.System.API.CSharp",
  "tags": {
    "language": "C#",
    "type": "solution"
  },
  "preferNameDirectory": true,
  "symbols": {
    "Framework": {
      "type": "parameter",
      "description": "The target framework for the project.",
      "datatype": "choice",
      "choices": [
        {
          "choice": "net6.0",
          "description": "Target net6.0"
        },
        {
          "choice": "net5.0",
          "description": "Target net5.0"
        }
      ],
      "replaces": "net6.0",
      "defaultValue": "net6.0"
    },
    "DB": {
     "type": "parameter",
     "description": "Database engine that solution will be connected to.",
     "datatype": "choice",
     "choices": [
       {
         "choice": "SQLServer",
         "description": "Target net5.0"
       },
       {
         "choice": "MongoDB",
         "description": "Target net5.0"
       }
     ],
     "defaultValue": "SQLServer"
    },
    "UseMongoDB": {
     "type": "computed",
     "value": "(DB == \"MongoDB\")"
    }
